<Application
    x:Class="RevitFamilyBuilder.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:RevitFamilyBuilder"
    StartupUri="MainWindow.xaml">


    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>

                <!--  // LIST RESOURCE DICTIONARIES BELOW //  -->
                <ResourceDictionary Source="Resources/Styles/CheckBoxStyles.xaml" />
                <ResourceDictionary Source="Resources/Styles/TextBlockStyles.xaml" />
                <ResourceDictionary Source="Resources/Styles/TextBoxStyles.xaml" />
                <ResourceDictionary Source="Resources/Styles/ButtonStyles.xaml" />
                <ResourceDictionary Source="Resources/Styles/SliderStyles.xaml" />
                <ResourceDictionary Source="Resources/Styles/BorderStyles.xaml" />
                <ResourceDictionary Source="Resources/Styles/NumericUpDownStyle.xaml" />
                <ResourceDictionary Source="Resources/Styles/RepeatButtonStyle.xaml" />

            </ResourceDictionary.MergedDictionaries>

            <!--<converters:SliderPositionConverter x:Key="SliderPositionConverter" />-->

        </ResourceDictionary>
    </Application.Resources>
</Application>
